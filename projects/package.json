{
  "name": "my-collection",
  "version": "0.0.0",
  "description": "A blank schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "build:watch": "tsc -p tsconfig.json --watch",
    "logging": "schematics .:my-logging --debug=false",
    "logging:debug": "schematics .:my-logging",
    "create.file": "schematics .:create-file --debug=false",
    "create.file:debug": "schematics .:create-file"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^12.2.9",
    "@angular-devkit/schematics": "^12.2.9",
    "typescript": "~4.3.2"
  },
  "devDependencies": {
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.8.0",
    "jasmine": "^3.5.0"
  }
}
